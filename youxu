  class Solution {  public:    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {        //避免元素覆盖  从后向前来 顺序从大到小        int len=nums1.size()-1;//nums1的大小        int size1=m-1;//nums1的最后一个元素的下标        int size2=n-1;//nums2的最后一个元素的下标           while(size2>=0)//因为是把nums2的元素往nums1中移动     {       if(size1<0)             nums1[len--]=nums2[size2--];       else if (nums1[size1]<nums2[size2])            nums1[len--]=nums2[size2--];       else            nums1[len--]=nums1[size1--];     }    }};
